{
  "name": "InkNest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "xcode": "xed -b ios",
    "pod": "npx pod-install",
    "ios-16": "react-native run-ios --simulator='iPhone 16'",
    "clean": "watchman watch-del-all",
    "clean:cache": "yarn cache clean && rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/Podfile.lock && rm -rf android/.gradle && rm -rf android/app/build && rm -rf android/build && yarn install && cd ios && pod install && cd ..",
    "build:release": "./script/gradle.sh && cd android && ./gradlew assembleRelease && cd .. && git checkout android/gradle.properties",
    "submodule": "sh ./script/git-submodule.sh",
    "setup": "yarn submodule && sh ./script/setup.sh"
  },
  "dependencies": {
    "@dr.pogodin/react-native-fs": "^2.36.2",
    "@invertase/react-native-apple-authentication": "^2.5.1",
    "@react-native-community/netinfo": "^11.5.2",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "axios": "^1.13.5",
    "lucide-react-native": "^0.563.0",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-gesture-handler": "3.0.0-beta.1",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.23.0",
    "react-native-svg": "^15.15.3",
    "react-native-worklets": "^0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
